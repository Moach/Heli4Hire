       "_cargoRandomDir = 5;" \n
       "_cargoDis = 6;" \n
       "_cargoCountOffset = (_cargoCount + 1) / 2;" \n
       "_listCargos = [];" \n
       "_listSpawns = [];" \n
       "for ""_i"" from 1 to _cargoCount do {" \n
       "	_ii = _i - _cargoCountOffset;" \n
       "" \n
       "	_posStart = _startSite modeltoworld [-10 + _ii * _cargoDis,0,12.2];" \n
