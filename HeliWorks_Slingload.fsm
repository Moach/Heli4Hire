       "		_rewardCoef = _rewardCoef - (_rewardCoefDef / _cargoCount);" \n
       "	};" \n
       "} foreach _listCargos;" \n
       "" \n
       "if (_rewardCoef < 0.001) then {_rewardCoef = 0;};" \n
       "" \n
       "if (_rewardCoef > 0) then {" \n
       "	""SLINGLOAD_OUTRO"" spawn bis_fnc_missionConversations;      " \n
       "} else {" \n
       "     // something" \n
       "};"/*%FSM</STATEINIT""">*/;
