    class Mission_Rewards
    {
      name = "Mission_Rewards";
      init = /*%FSM<STATEINIT""">*/"//--- Evaluate reward" \n
       "_rewardCoef = 1;" \n
       "" \n
       "_rewardCoefDef = _rewardCoef;" \n
       "{" \n
       "	if (_x getvariable [""BIS_slingload_lost"",false]) then {" \n
       "" \n
