       "				};" \n
       "			};" \n
       "			_spawns set [count _spawns,_spawn];" \n
       "		};" \n
       "	} foreach _cranes;" \n
       "} foreach [_startSite,_endSite];" \n
       "" \n
       "#define CARGO_LOST (_cargo getvariable [""BIS_slingload_lost"",false])" \n
